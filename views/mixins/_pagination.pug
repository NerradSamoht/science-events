mixin pagination(page, pages, count, parentSlug)
	.pagination
		.pagination__prev
			if page > 1
				a(href=`/${parentSlug}/page/${page - 1}`) Prev
		.pagination__text
			p Page #{page} of #{pages} - #{count} total results
		.pagination__next
			if page < pages
				a(href=`/${parentSlug}/page/${parseFloat(page) + 1}`) Next
